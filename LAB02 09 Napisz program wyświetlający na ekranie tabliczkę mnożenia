//5. Napisz program wyświetlający na ekranie tabliczkę mnożenia


#include <iostream>

using namespace std;
const unsigned int SZEROKOSC=4;
int main() {
    for(int i=1;i<11;i++) {
        if (i>1) cout<<endl;
        for(int j=1;j<11;j++) {
            cout.width(SZEROKOSC); cout<<i*j;
        }
    }
    cout<<endl;
    return 0;
}
