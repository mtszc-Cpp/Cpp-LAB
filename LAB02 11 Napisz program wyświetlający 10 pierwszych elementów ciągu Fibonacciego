//7. Napisz program wyświetlający 10 pierwszych elementów ciągu Fibonacciego

#include <iostream>

using namespace std;

int main(){
    int fib[1000], n;
    cout << "podaj liczbe: ";
    cin >> n;
    fib[0] = 1;
    fib[1] = 1;
    for (int i = 2; i<n;i++)
        fib[i]=fib[i-1]+fib[i-2];
    for (int i = 0; i<n;i++)
       cout<<fib[i];
    return(0);
}
